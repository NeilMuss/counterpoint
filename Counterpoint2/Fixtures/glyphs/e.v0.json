{
  "render": {
    "canvasPx": { "width": 1600, "height": 1600 },
    "fitMode": "none",
    "paddingWorld": 0,
    "clipToFrame": false,
    "worldFrame": {
      "minX": -250,
      "minY": -250,
      "maxX": 1250,
      "maxY": 1250
    }
  },

  "reference": {
    "path": "Fixtures/references/big_caslon_e.svg",
    "translateWorld": { "x": 0, "y": 0 },
    "scale": 1.0,
    "rotateDeg": 0,
    "opacity": 0.25,
    "lockPlacement": true
  },

  "ink": {

    "upper_loop": {
      "type": "cubic",
      "p0": { "x": 350, "y": 166 },
      "p1": { "x": 350, "y": -40 },
      "p2": { "x": 70,  "y": -40 },
      "p3": { "x": 60,  "y": 200 }
    },

    "lower_loop": {
      "type": "cubic",
      "p0": { "x": 60,  "y": 200 },
      "p1": { "x": 50,  "y": 470 },
      "p2": { "x": 310, "y": 480 },
      "p3": { "x": 395, "y": 310 }
    },

    "e_bowl": {
      "type": "heartline",
      "parts": ["upper_loop", "lower_loop"]
    },


    "cross": {
      "type": "line",
      "p0": { "x": 75,  "y": 166 },
      "p1": { "x": 375, "y": 166 }
    }
  },

  "strokes": [


    {
      "id": "e-bowl",
      "type": "stroke",
      "ink": "e_bowl",

      "params": {
        "angleMode": "relative",

        "theta": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        },

        "widthLeft": {
          "keyframes": [
            { "t": 0.0,  "value": 40 },
            { "t": 0.25, "value": 22 },
            { "t": 0.55, "value": 62 },
            { "t": 0.80, "value": 30 },
            { "t": 1.0,  "value": 14 }
          ]
        },

        "widthRight": {
          "keyframes": [
            { "t": 0.0,  "value": 95},
            { "t": 0.1,  "value": 83 },
            { "t": 0.25, "value": 26 },
            { "t": 0.55, "value": 72 },
            { "t": 0.80, "value": 60 },
            { "t": 1.0,  "value": 18 }
          ]
        },

        "offset": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        }
      }
    },


    {
      "id": "e-cross",
      "type": "stroke",
      "ink": "cross",

      "params": {
        "angleMode": "relative",

        "theta": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        },

        "widthLeft": {
          "keyframes": [
            { "t": 0.0, "value": 18 },
            { "t": 1.0, "value": 18 }
          ]
        },

        "widthRight": {
          "keyframes": [
            { "t": 0.0, "value": 18 },
            { "t": 1.0, "value": 18 }
          ]
        },

        "offset": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        }
      }
    }
  ]
}
