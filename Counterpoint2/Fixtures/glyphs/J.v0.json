{
  "render": {
    "canvasPx": { "width": 1600, "height": 1600 },
    "fitMode": "none",
    "paddingWorld": 0,
    "clipToFrame": false,
    "worldFrame": {
      "minX": -250,
      "minY": -250,
      "maxX": 1250,
      "maxY": 1250
    }
  },

  "reference": {
    "path": "Fixtures/references/big_caslon_J.svg",
    "translateWorld": { "x": 0, "y": 0 },
    "scale": 1.0,
    "rotateDeg": 0,
    "opacity": 0.25,
    "lockPlacement": true
  },

  "ink": {
    "stem": {
      "type": "line",
      "p0": { "x": 138, "y": 5 },
      "p1": { "x": 138, "y": 350 }
    },

    "hook": {
      "type": "cubic",
      "p0": { "x": 138, "y": 350 },
      "p1": { "x": 138, "y": 450 },
      "p2": { "x": -10, "y": 500 },
      "p3": { "x": 30, "y": 420 }
    },

    "J_heartline": {
      "type": "heartline",
      "parts": ["stem", "hook"]
    }
  },

  "strokes": [
    {
      "id": "J-main",
      "type": "stroke",
      "ink": "J_heartline",

      "params": {
        "angleMode": "relative",

        "theta": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        },

        "widthLeft": {
          "keyframes": [
            { "t": 0.0, "value": 35 },
            { "t": 0.7, "value": 35 },

            { "t": 0.9, "value": 5 },
            { "t": 1.0, "value": 20 }
          ]
        },

        "widthRight": {
          "keyframes": [
            { "t": 0.0, "value": 35 },
            { "t": 0.7, "value": 35 },

            { "t": 0.9, "value": 5 },
            { "t": 1.0, "value": 20 }
          ]
        }
        ,

        "offset": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        }
      }
    }
  ]
}
