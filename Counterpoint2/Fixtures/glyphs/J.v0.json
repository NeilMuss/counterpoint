{
  "render": {
    "canvasPx": { "width": 1600, "height": 1600 },
    "fitMode": "none",
    "paddingWorld": 0,
    "clipToFrame": false,
    "worldFrame": {
      "minX": -250,
      "minY": -250,
      "maxX": 1250,
      "maxY": 1250
    }
  },

  "reference": {
    "path": "Fixtures/references/big_caslon_J.svg",
    "translateWorld": { "x": 0, "y": 0 },
    "scale": 1.0,
    "rotateDeg": 0,
    "opacity": 0.25,
    "lockPlacement": true
  },

  "ink": {
    "stem": {
      "type": "line",
      "p0": { "x": 138, "y": 0 },
      "p1": { "x": 138, "y": 350 }
    },

    "hook": {
      "type": "cubic",
      "p0": { "x": 138, "y": 350 },
      "p1": { "x": 138, "y": 470 },
      "p2": { "x": 0, "y": 480 },
      "p3": { "x": 30, "y": 420 }
    },

    "J_heartline": {
      "type": "heartline",
      "parts": ["stem", "hook"]
    }
  },

  "strokes": [
    {
      "id": "J-main",
      "type": "stroke",
      "ink": "J_heartline",

      "params": {
        "angleMode": "relative",

        "theta": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        },

        "widthLeft": {
          "keyframes": [
            { "t": 0.0, "value": 99 },
            { "t": 0.013, "value": 99 },
            { "t": 0.2, "value": 35 },
            { "t": 0.7, "value": 35  },
            { "t": 0.8, "value": 15  },
            { "t": 0.85, "value": 5  },
            { "t": 0.9, "value": 3 },
            { "t": 0.96, "value": 6, "interpToNext": { "alpha": -1.6 }  },
            { "t": 1.0, "value": 30 }
          ]
        },

        "widthRight": {
          "keyframes": [
            { "t": 0.0, "value": 94  },
            { "t": 0.012, "value": 94, "interpToNext": { "alpha": -1.6 }  },
            { "t": 0.10, "value": 35 },
            { "t": 0.7, "value": 35},
            { "t": 0.8, "value": 14},
            { "t": 0.9, "value": 3 },
            { "t": 0.95, "value": 10 },
            { "t": 1.0, "value": 33 }
          ]
        }
        ,

        "offset": {
          "keyframes": [
            { "t": 0.0, "value": 0 },
            { "t": 1.0, "value": 0 }
          ]
        },

        "alpha": {
          "keyframes": [
            { "t": 0.0, "value": 0.0 },
            { "t": 0.7, "value": 0.6 },
            { "t": 1.0, "value": 0.0 }
          ]
        }

      }
    }
  ]
}
